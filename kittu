
#include <stdio.h>
void main()
{
    int a,b;
    printf("enter the num1:");
    scanf("%d",&a);
    printf("enter the num2:");
    scanf("%d",&b);
    int big=a>b? a:b;
    int step=big;
    while(1)
    {
        if(big %2==1 && big%b==0)
        {
            printf("the lcm is %d",big);
            break;
        }
        else
        big=big+step
        
    
    



